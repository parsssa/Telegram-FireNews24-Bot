<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane fx:id="backgroundFinestrina" prefHeight="242.0" prefWidth="351.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.inferno_fx.ControllerModificaUtente">
   <children>
      <Label fx:id="titoloUtente" alignment="TOP_CENTER" layoutX="36.0" layoutY="6.0" prefHeight="89.0" prefWidth="283.0" text="Modifica Utente" wrapText="true">
         <font>
            <Font name="Consolas" size="16.0" />
         </font></Label>
      <TextField fx:id="UsernameBox" layoutX="177.0" layoutY="102.0" />
      <Text fill="WHITE" layoutX="24.0" layoutY="119.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Nuovo Username" />
      <Text fill="WHITE" layoutX="24.0" layoutY="183.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Nuova Password" />
      <TextField fx:id="PasswordBox" layoutX="177.0" layoutY="166.0" />
      <ImageView fx:id="floppyDisk" fitHeight="25.0" fitWidth="25.0" layoutX="339.0" layoutY="2.0" onMouseClicked="#salvataggio" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@save.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="25.0" fitWidth="25.0" layoutX="2.0" layoutY="2.0" onMouseClicked="#switchToUserManager" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@VariePNG/backButton.png" />
         </image>
      </ImageView>
      <Label fx:id="logMessage" alignment="CENTER" layoutX="19.0" layoutY="205.0" prefHeight="17.0" prefWidth="337.0" />
   </children>
</AnchorPane>
